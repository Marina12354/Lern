.1 {
    height: 120px;
    width: 171px;
    background-image: url('../img/1.png');
    display: block;
   
}
.2 {
    width: 131px;
    background-image: url('../img/2.png');
    display: block;
    height: 125px;
    background-position: center center;
    background-repeat: no-repeat;
}
.3 {
    width: 147px;
